# PostgreSQL
POSTGRES_HOST=localhost
POSTGRES_PORT=5546
POSTGRES_DB=ezpocket_logs
POSTGRES_USER=ezpocket_user
POSTGRES_PASSWORD=your_postgres_password_here

# Redis (Sistema de Filas)
REDIS_HOST=localhost
REDIS_PORT=6493
REDIS_DB=0

# OpenAI
OPENAI_API_KEY=sk-proj-your_openai_api_key_here

# AWS Athena
AWS_ACCESS_KEY=your_aws_access_key_here
AWS_SECRET_KEY=your_aws_secret_key_here
AWS_REGION=us-east-1
ATHENA_OUTPUT_S3=s3://your-bucket-name/
ATHENA_DATABASE=your_athena_database

# Application Ports
MAIN_APP_PORT=8000
INTENT_VALIDATOR_PORT=5001
HISTORY_PREFERENCES_PORT=5002
ROUTER_PORT=5005
GENERATOR_PORT=5006
RESPONDER_PORT=5007
GRAPH_ORCHESTRATOR_PORT=5008

# Frontend Mode (development ou production)
FRONTEND_MODE=production
PLAN_BUILDER_PORT=5009
PLAN_CONFIRM_PORT=5010
USER_PROPOSED_PLAN_PORT=5011
ANALYSIS_ORCHESTRATOR_PORT=5012
PLAN_REFINER_PORT=5013
SQL_VALIDATOR_PORT=5014
AUTO_CORRECTION_PORT=5015
ATHENA_EXECUTOR_PORT=5017
PYTHON_RUNTIME_PORT=5018
FLOW_ORCHESTRATION_PORT=5004

# Job Cleanup Configuration (em segundos)
JOB_CLEANUP_TIMEOUT=300  # 5 minutos = 300 segundos

# Data Sync Configuration
DATA_SYNC_PORT=5019
DATA_SYNC_SCHEDULE="20 * * * *"  # Executar a cada 20 minutos de cada hora (14:20, 15:20, ...)
DATA_SYNC_ATHENA_TABLE=report_orders
DATA_SYNC_POSTGRES_TABLE=order_report
DATA_SYNC_BATCH_SIZE=1000
DATA_SYNC_MAX_RETRIES=3       # Máximo de tentativas se falhar
DATA_SYNC_RETRY_DELAY=300     # Delay entre tentativas (segundos)

# Origem dos Dados
BD_REFERENCE=Local         # Athena ou Local - define qual banco de dados será usado (Athena ou PostgreSQL Local)

# Keycloak
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=your_keycloak_admin_password
KEYCLOAK_PORT=8090
KEYCLOAK_HOSTNAME=localhost
KEYCLOAK_REALM=ezpocket-realm
KEYCLOAK_CLIENT_ID=ezpocket-client
KEYCLOAK_CLIENT_SECRET=your_keycloak_client_secret_here

# Security
SECURE_COOKIES=false  # true para HTTPS em produção, false para HTTP em desenvolvimento

# Redis Commander
REDIS_COMMANDER_USER=Admin
REDIS_COMMANDER_PASSWORD=your_redis_commander_password_here
